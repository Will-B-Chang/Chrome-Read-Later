{
   "background": {
      "persistent": false,
      "page": "src/html/background.html"
   },
   "browser_action": {
      "default_icon": "src/images/32x32orange.png",
      "default_popup": "src/html/popup.html",
      "default_title": "Read Later"
   },
   "commands": {
      "read-later": {
         "description": "Add to reading list",
         "suggested_key": {
            "default": "Alt+Shift+S"
         }
      }
   },
   "description": "Read current tab or selected link later.",
   "icons": {
      "128": "src/images/128x128orange.png",
      "16": "src/images/16x16orange.png",
      "32": "src/images/32x32orange.png",
      "48": "src/images/48x48orange.png"
   },
   "manifest_version": 2,
   "name": "Read Later",
   "permissions": [ "tabs", "storage", "contextMenus" ],
   "version": "1.2.4"
}
