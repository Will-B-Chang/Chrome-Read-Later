{
  "background": {
    "persistent": true,
    "page": "feat_background/index.html"
  },
  "browser_action": {
    "default_icon": "assets/icons/logo-orange32x32.png",
    "default_popup": "feat_popup/index.html",
    "default_title": "Read Later"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "feat_content/index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "modules/page.mjs"
  ],
  "commands": {
    "read-later": {
      "description": "Add to reading list",
      "suggested_key": {
        "default": "Alt+Shift+S"
      }
    },
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+Z"
      }
    }
  },
  "description": "Read current tab or selected link/page later.",
  "icons": {
    "128": "assets/icons/logo-orange128x128.png",
    "16": "assets/icons/logo-orange16x16.png",
    "32": "assets/icons/logo-orange32x32.png",
    "48": "assets/icons/logo-orange48x48.png"
  },
  "manifest_version": 2,
  "name": "Read Later",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "contextMenus",
    "notifications"
  ],
  "version": "2.3.0"
}
